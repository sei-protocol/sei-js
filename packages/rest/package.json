{
  "name": "@sei-js/rest",
  "version": "0.0.0-internal-evm-20240311234659",
  "description": "A library to build and host the REST API documentation for Sei",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist && yarn fetch-openapi && yarn md-docs",
    "fetch-openapi": "curl -o sei-openapi.yml https://raw.githubusercontent.com/sei-protocol/sei-chain/main/docs/static/openapi.yml",
    "md-docs": "widdershins sei-openapi.yml -o openapi.md --omitHeader --c",
    "build": "tsc --project tsconfig.json",
    "docs": "typedoc --out docs"
  },
  "homepage": "https://github.com/sei-protocol/sei-js#readme",
  "keywords": [
    "sei",
    "REST",
    "documentation"
  ],
  "repository": "git@github.com:sei-protocol/sei-js.git",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "widdershins": "^4.0.1"
  },
  "peerDependencies": {},
  "devDependencies": {},
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    }
  }
}
