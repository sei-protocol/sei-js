  "author": "Carson Aberle",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@sei-js/proto": "*"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0"
  }
}
