{
	"linter": {
		"rules": {
			"correctness": {
				"noUnusedImports": "error"
			},
			"complexity": {
				"noStaticOnlyClass": "off",
				"useLiteralKeys": "off",
				"noBannedTypes": "off"
			}
		}
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "double",
			"jsxQuoteStyle": "double"
		}
	}
}
