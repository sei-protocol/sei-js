{
  "name": "@sei-js/ledger",
  "version": "1.1.0",
  "description": "TypeScript library for SEI Ledger app helper functions",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
